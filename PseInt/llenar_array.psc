Algoritmo llenar_array
	Escribir "Llenar array de 0 a 10";
	Dimension arr[11];
	Para i<-0 Hasta 10 Con Paso 1 Hacer
		Escribir "Numero en posicion numero ", i, ":";
		Leer arr[i];
	Fin Para
	Para x <- 0 Hasta 10 Con Paso 1 Hacer;
		Escribir "Posicion ", x, ":", arr[x];
	FinPara
FinAlgoritmo
