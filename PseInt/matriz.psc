Algoritmo matriz
	Dimension arr[3, 3];
	Para i<-0 Hasta 2 Con Paso 1 Hacer
		Para x<-0 Hasta 2 Con Paso 1 Hacer
			cont <- cont + 1;
			arr[i, x] <- cont;
		Fin Para
	Fin Para
	Para j<-0 Hasta 2 Con Paso 1 Hacer
		Para k<-0 Hasta 2 Con Paso 1 Hacer
			Escribir arr[j,k];
		Fin Para
	Fin Para
FinAlgoritmo
